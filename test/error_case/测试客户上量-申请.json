{
  "name": "formUsrTest",
  "initData": {
      "specialBandwidth": 0,
      "viewType": "cn",
      "addFlowType": "suddenlly"
  },
  "models": {
    "getUsers": {
      "action": "auto",
      "headers": {
        "token": "@workflowToken"
      },
      "type": "restful",
      "url": "http://kunlun.baishancloud.com/ssoClient/api_portal_user/getPortalUserList ",
      "params": {},
      "streams": {
        "$userList": {
          "path": "data",
          "alias": {
            "label": "name",
            "value": "id"
          }
        }
      }
    },
    "getStaffs": {
      "action": "auto",
      "type": "restful",
      "headers": {
        "token": "@workflowToken"
      },
      "url": "http://kunlun.baishancloud.com/ssoClient/api_user/getUserList",
      "params": {},
      "streams": {
        "$staffList": {
          "path": "info",
          "alias": {
            "label": "email",
            "value": "id"
          }
        }
      }
    }
  },
  "global": {
    "staffList": [],
    "userList": []
  },
  "layout": [
    {
      "component": "Row",
      "children": [
        {
          "col": 24,
          "component": "h6",
          "text": "提示：上量带宽<=1G，且海外与小运营商上量带宽<=100M则无需申请；1~5G可当天申请；5~50G需提前一天；50~100G需提前一周；100G以上需提前两周。",
          "props":{
            "style":{"color": "red"}
          }
        },
        {
          "col": 24,
          "component": "Card",
          "props": {
            "title": "常规信息"
          },
          "children": [
            {
              "component": "Form",
              "props": {
                "colWidth": 8,
                "labelCol": 13,
                "wrapperCol": 11,
                "gutter": 30
              },
              "children": [
                {
                  "col": 6,
                  "name": "usr",
                  "label": "客户",
                  "component": "Select",
                  "rules": [
                    {
                      "required": true,
                      "message": "请选择客户"
                    }
                  ],
                  "props": {
                    "options": "$userList"
                  }
                },
                {
                  "name": "totalBandwidth",
                  "label": "总新增上量大小(Gbps)",
                  "component": "InputNumber",
                  "rules": [
                    {
                      "required": true,
                      "message": "请输入带宽量"
                    }
                  ]
                },
                {
                  "col": 10,
                  "name": "specialBandwidth",
                  "label": "海外/小运营商上量(Gbps)",
                  "component": "InputNumber",
                  "rules": [
                    {
                      "required": true,
                      "message": "请输入带宽量"
                    }
                  ]
                },
                {
                  "col": 7,
                  "label": "上量开始时间",
                  "component": "DatePicker",
                  "name": "startdate",
                  "rules": [
                    {
                      "required": true,
                      "message": "请填写上量开始时间"
                    }
                  ]
                },
                {
                  "col": 7,
                  "label": "预计结束时间",
                  "component": "DatePicker",
                  "name": "enddate"
                },
                {
                  "col": 5,
                  "label": "上量区域",
                  "component": "Select",
                  "name": "viewType",
                  "props":{
                  "options": [
                    {
                      "label": "国内",
                      "value": "cn"
                    },
                    {
                      "label": "海外",
                      "value": "ov"
                    },
                    {
                      "label": "全网",
                      "value": "global"
                    }
                  ]
                }
                },
                {
                  "col": 8,
                  "label": "上量模式",
                  "component": "Select",
                  "name": "addFlowType",
                  "props":{

                  "options": [
                    {
                      "label": "一次性切量",
                      "value": "suddenlly"
                    },
                    {
                      "label": "逐步增加",
                      "value": "gradually"
                    }
                  ]
                }
                },
                {
                  "name": "saler",
                  "label": "对应的销售",
                  "component": "Select",
                  "props": {
                    "options": "$staffList"
                  },
                  "rules": [
                    {
                      "required": true,
                      "message": "请选择销售"
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "component": "Row",
      "children": [
        {
          "col": 24,
          "component": "Card",
          "props": {
            "title": "详细信息"
          },
          "children": [
            {
              "name": "detail",
              "component": "EditTable",
              "props": {
                "columns": [
                  {
                    "title": "域名",
                    "component": "Input",
                    "name": "domain",
                    "width": "20%",
                    "rules": [
                      {
                        "required": false,
                        "message": "请填写域名"
                      }
                    ]
                  },
                  {
                    "title": "类型",
                    "component": "Select",
                    "name": "domainType",
                    "width": "10%",
                    "rules": [
                      {
                        "required": false,
                        "message": "请选择业务类型"
                      }
                    ],
                    "props":{
                    "options": [
                      {
                        "label": "网页",
                        "value": "page"
                      },
                      {
                        "label": "下载",
                        "value": "download"
                      },
                      {
                        "label": "点播",
                        "value": "demand"
                      },
                      {
                        "label": "直播",
                        "value": "live"
                      },
                      {
                        "label": "动态",
                        "value": "dynamic"
                      },
                      {
                        "label": "其他",
                        "value": "other"
                      }
                    ]
                  }
                    },
                    {
                      "title": "覆盖链",
                      "component": "input",
                      "name": "cover",
                      "width": "15%",
                      "rules": [
                        {
                          "required": false,
                          "message": "请填写覆盖链名"
                        }
                      ]
                    },
                    {
                      "title": "地区运营商",
                      "component": "input",
                      "name": "view",
                      "width": "25%",
                      "rules": [
                        {
                          "required": false,
                          "message": "请填写地区运营商"
                        }
                      ]
                    },
                    {
                      "title": "新增上量大小(Gbps)",
                      "component": "InputNumber",
                      "name": "bandwidth",
                      "width": "17%"
                    },
                    {
                      "title": "备注",
                      "component": "Input",
                      "name": "remark",
                      "width": "17%"
                    }
                  ]
                }
            }
          ]
        }
      ]
    },
    {
      "component": "Row",
      "children": [
        {
          "col": 24,
          "component": "Card",
          "props": {
            "title": "备注"
          },
          "children": [
            {
              "name": "specialDesc",
              "component": "Editor"
            }
          ]
        }
      ]
    }
  ]
}